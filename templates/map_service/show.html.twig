{% extends 'base.html.twig' %}

{% block title %}MapService{% endblock %}

{% block body %}
    <h1>MapService</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ map_service.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ map_service.Name }}</td>
            </tr>
            <tr>
                <th>Photo</th>
                <td>{{ map_service.Photo }}</td>
            </tr>
            <tr>
                <th>PostCode</th>
                <td>{{ map_service.PostCode }}</td>
            </tr>
            <tr>
                <th>Comment</th>
                <td>{{ map_service.Comment }}</td>
            </tr>
            <tr>
                <th>Service</th>
                <td>{{ map_service.Service }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ map_service.Status ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Coordinate</th>
                <td>{{ map_service.coordinate }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('map_service_index') }}">back to list</a>

    <a href="{{ path('map_service_edit', {'id': map_service.id}) }}">edit</a>

    {{ include('map_service/_delete_form.html.twig') }}
{% endblock %}
